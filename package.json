{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "sayho",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --logHeapUsage --no-compilation-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit --logHeapUsage --no-compilation-cache",
    "env:up": "oci os object put -bn environments --file .env/.env.test --name nest-boilerplate/.env.test --no-multipart && oci os object put -bn environments --file .env/.env.development --name nest-boilerplate/.env.development --no-multipart && oci os object put -bn environments --file .env/.env.production --name nest-boilerplate/.env.production --no-multipart",
    "env:down": "oci os object get -bn environments --file .env/.env.test --name nest-boilerplate/.env.test && oci os object get -bn environments --file .env/.env.development --name nest-boilerplate/.env.development && oci os object get -bn environments --file .env/.env.production --name nest-boilerplate/.env.production",
    "docker-compose:up": "docker-compose -f docker-compose.yml up --build -d",
    "docker-compose:down": "docker-compose -f docker-compose.yml down --volumes",
    "typeorm": "NODE_ENV=test && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d=typeorm.config.ts",
    "typeorm:create": "pnpm typeorm migration:create",
    "typeorm:generate": "pnpm typeorm migration:generate",
    "typeorm:show": "pnpm typeorm migration:show",
    "typeorm:up": "pnpm typeorm migration:run",
    "typeorm:down": "pnpm typeorm migration:revert",
    "typeorm-ext": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs -d typeorm.config.ts",
    "db:up": "pnpm typeorm-ext db:create",
    "db:down": "pnpm typeorm-ext db:drop",
    "seed:run": "pnpm typeorm-ext seed:run"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.7.1",
    "nest-winston": "^1.9.4",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dotenv": "^16.3.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-extension": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json",
          "isolatedModules": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/../"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
